{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "project": [
    "plugins/**/*.{ts,tsx}",
    "scripts/**/*.{ts,tsx}"
  ],
  "typescript": { "config": ["tsconfig.json"] },
  "ignore": [
    ".github/**",
    "node_modules/**",
    "scripts/zip/compileProduction.ts",
    "scripts/githooks/**",
    "scripts/init/**"
  ],
  "ignoreFiles": [
    "plugins/**"],
  "rules": {
    "exports": "error",
    "nsExports": "error",
    "files": "error"
  }
}
