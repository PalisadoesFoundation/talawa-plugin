{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "scripts/**/*.{ts,js}",
    "docs/src/pages/**/*.{ts,tsx}",
    "docs/docusaurus.config.ts",
    "docs/sidebars.ts"
  ],
  "project": [
    "plugins/**/*.{ts,tsx}",
    "scripts/**/*.{ts,tsx}",
    "docs/**/*.{ts,tsx,md,mdx}"
  ],
  "typescript": { "config": ["tsconfig.json"] },
  "ignore": [
    ".github/**",
    "node_modules/**",
    "scripts/zip/compileProduction.ts",
    "scripts/githooks/**",
    "scripts/init/infoDocker.ts",
    "scripts/init/infoName.ts",
    "scripts/init/initAdmin.ts",
    "scripts/init/initApi.ts",
    "scripts/init/initApp.ts",
    "scripts/init/initDocker.ts",
    "plugins/**"
  ],
  "ignoreFiles": ["plugins/**"],
  "ignoreExportsUsedInFile": true,
  "rules": {
    "exports": "error",
    "nsExports": "error",
    "files": "error"
  }
}
